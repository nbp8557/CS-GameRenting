<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="entities.Console" table="Console">
		<meta attribute="class-description">
			This class contains the console detail.
		</meta>
		
		<id name="consoleID" type="int" column="ConsoleID">
			<generator class="native" />
		</id>
		
		<property name="consoleName" column="ConsoleName" type="string" />

	</class>
</hibernate-mapping>